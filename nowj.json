{
  "version": 2,
  "builds": [
    {
      "src": "virxcase/wsgi.py",
      "use": "@ardnt/now-python-wsgi",
      "config": { "maxLambdaSize": "20mb" }
    },
    {
      "src": "main/static/(.*)",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "main/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "virxcase/wsgi.py"
    }
  ]
}